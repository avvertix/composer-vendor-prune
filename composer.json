{
    "name": "avvertix/composer-vendor-prune",
    "description": "Clean the PHP Composer vendor folder from files not needed in production",
    "type": "library",
    "license": "MIT",
    "bin": ["bin/vendor-prune"],
    "authors": [
        {
            "name": "Alessio",
            "email": "alessio@vertemati.space"
        }
    ],
    "autoload": {
        "psr-4": {
            "Avvertix\\VendorPrune\\": "src/"
        }
    },
	"autoload-dev": {
		"psr-4": {
            "Avvertix\\VendorPrune\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/var-dumper": "^4.1"
    }
}
